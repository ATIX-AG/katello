# Place this file in your /etc/yum.repos.d/ directory

[katello]
name=Katello Stable
baseurl=http://fedorapeople.org/groups/katello/releases/yum/1.0/@SUBDIR@/$releasever/$basearch/
enabled=1
skip_if_unavailable=1
gpgcheck=0

[katello-source]
name=Katello Stable Source
baseurl=http://fedorapeople.org/groups/katello/releases/source/1.0/@SUBDIR@/$releasever/
enabled=0
skip_if_unavailable=1
gpgcheck=0

